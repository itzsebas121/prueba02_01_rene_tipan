<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    android:orientation="vertical">

    <EditText android:id="@+id/rstn_nombre"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Nombre" />

    <EditText android:id="@+id/rstn_apellido"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Apellido" />

    <EditText android:id="@+id/rstn_dividendo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Dividendo"
        android:enabled="false" />

    <EditText android:id="@+id/rstn_divisor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Divisor"
        android:enabled="false" />

    <EditText android:id="@+id/rstn_numero"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="NÃºmero"
        android:enabled="false" />

    <Button android:id="@+id/rstn_btn_siguiente_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Siguiente" />

    <Button android:id="@+id/rstn_btn_cerrar_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cerrar" />
</LinearLayout>
