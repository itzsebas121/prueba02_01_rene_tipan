<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp">

    <EditText android:id="@+id/rstn_nombre"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Nombre"
        android:enabled="false" />

    <EditText android:id="@+id/rstn_apellido"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Apellido"
        android:enabled="false" />

    <EditText android:id="@+id/rstn_dividendo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Dividendo" />

    <EditText android:id="@+id/rstn_divisor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Divisor" />

    <EditText android:id="@+id/rstn_numero"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="NÃºmero" />

    <Button android:id="@+id/rstn_btn_cerrar_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cerrar" />
</LinearLayout>
